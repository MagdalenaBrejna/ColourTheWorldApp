<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.HBox?>

<HBox xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.magdalena.brejna.colourtheworldapp.controllers.ProjectButtonsController">
   <children>
      <Button mnemonicParsing="false" text="%choose.button">
         <HBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" text="%create.button">
         <HBox.margin>
            <Insets bottom="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" text="%saveAs.button">
         <HBox.margin>
            <Insets bottom="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" text="%delete.button">
         <HBox.margin>
            <Insets bottom="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" text="%close.button">
         <HBox.margin>
            <Insets bottom="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </Button>
      <Button mnemonicParsing="false" text="%newWindow.button">
         <HBox.margin>
            <Insets bottom="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
