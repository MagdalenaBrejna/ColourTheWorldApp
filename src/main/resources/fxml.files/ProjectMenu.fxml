<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.geometry.Insets?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.control.MenuButton?>
<?import javafx.scene.control.MenuItem?>
<?import javafx.scene.control.TextField?>
<?import javafx.scene.image.Image?>
<?import javafx.scene.image.ImageView?>
<?import javafx.scene.layout.HBox?>

<HBox prefHeight="70.0" prefWidth="699.0" xmlns="http://javafx.com/javafx/8.0.171" xmlns:fx="http://javafx.com/fxml/1" fx:controller="pl.magdalena.brejna.colourtheworldapp.controllers.ProjectMenuController">
   <children>
      <MenuButton mnemonicParsing="false" prefHeight="30.0" prefWidth="22.0" text=" ">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
         <HBox.margin>
            <Insets bottom="20.0" left="20.0" top="20.0" />
         </HBox.margin>
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/126486.png" />
               </image>
            </ImageView>
         </graphic>
      </MenuButton>
      <TextField prefHeight="30.0" prefWidth="230.0">
         <HBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </TextField>
      <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="%save.button">
         <HBox.margin>
            <Insets bottom="20.0" top="20.0" />
         </HBox.margin>
      </Button>
      <MenuButton mnemonicParsing="false" prefHeight="30.0" prefWidth="170.0" text="%menu.button">
        <items>
          <MenuItem mnemonicParsing="false" text="Action 1" />
          <MenuItem mnemonicParsing="false" text="Action 2" />
        </items>
         <HBox.margin>
            <Insets bottom="20.0" left="60.0" top="20.0" />
         </HBox.margin>
         <graphic>
            <ImageView fitHeight="20.0" fitWidth="20.0" pickOnBounds="true" preserveRatio="true">
               <image>
                  <Image url="@../icons/126474.png" />
               </image>
            </ImageView>
         </graphic>
      </MenuButton>
      <Button mnemonicParsing="false" prefHeight="30.0" prefWidth="60.0" text="%edit.button">
         <HBox.margin>
            <Insets bottom="20.0" left="20.0" right="20.0" top="20.0" />
         </HBox.margin>
      </Button>
   </children>
</HBox>
